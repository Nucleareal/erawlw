@CHEAT_SLAVE_STATUS_MARK
REPEAT 10
	SIF MARKNAME:COUNT == ""
		CONTINUE
	
	IF COUNT == 3
		MARK:3 = 0
		CONTINUE
	ENDIF
		
	MARK:COUNT = 3
REND
PRINTW 刻印を最大まで強化しました

@CHEAT_SLAVE_STATUS_EXP
REPEAT 100
	SIF EXPNAME:COUNT == ""
		CONTINUE
		
	SIF COUNT == 0 && TALENT:0
		CONTINUE
		
	;BL経験
	SIF COUNT == 41
		CONTINUE
	
	EXP:COUNT += 10000
REND
PRINTW 経験を1万加算しました

@CHEAT_SLAVE_STATUS_JUEL
REPEAT 13	
	JUEL:COUNT += 1000000
REND
PRINTW 珠を100万加算しました



@JUDGE_CHARACTER_IN_LIST, ARG, ARG:1

LOCAL = 0

REPEAT CHARANUM
	SIF COUNT == MASTER
		CONTINUE
		
	SIF ARG:1 && COUNT != ASSI
		CONTINUE
	
	SIF NO:COUNT == ARG
		LOCAL = LOCAL + 1
REND

RETURN LOCAL