@CALC_CALENDER(ARG:0)
#FUNCTION

VARSET LOCAL, 0

C_YEAR:0 = 0
C_MONTH:0 = 0
C_DAY:0 = 0

;DEBUGPRINTFORML LOCAL:{ARG:0}.

LOCAL:0 = ARG:0

;YEAR
WHILE LOCAL:0 >= 365
	LOCAL:0 = LOCAL:0 - 365
	C_YEAR:0 = C_YEAR:0 + 1
WEND

;0~ -> 1~
C_YEAR:1 = C_YEAR:0 + 1

;MONTH
REPEAT 12
	LOCAL:1 = 100 + COUNT + 1
	
	IF LOCAL:0 >= C_DAY:(LOCAL:1)
		LOCAL:0 -= C_DAY:(LOCAL:1)
		C_MONTH:0 = C_MONTH:0 + 1
	ELSE
		BREAK
	ENDIF
REND

;0~11 -> 1~12
C_MONTH:1 = C_MONTH:0 + 1

C_DAY:0 = LOCAL:0

;0~30 -> 1~31
C_DAY:1 = C_DAY:0 + 1

RETURNF 0

@DISPLAY_CALENDER, ARG:0

LOCAL = CALC_CALENDER(ARG:0)

PRINTFORM {C_YEAR:1,2}îN{C_MONTH:1,2}åé{C_DAY:1,2}ì˙ 

