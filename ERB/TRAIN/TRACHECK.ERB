;奉仕系である、奉仕快楽のチェック
E = 0

IF UP:7 < 100
	A = 0
ELSEIF UP:7 < 300
	A *= 1
ELSEIF UP:7 < 700
	A *= 2
ELSEIF UP:7 < 1500
	A *= 3
ELSE
	A *= 4
ENDIF

IF TFLAG:100
	IF A >= 12000
		E = 16
		TIMES UP:11 , 0.65
		TIMES UP:12 , 0.30
	ELSEIF A >= 8000
		E = 12
		TIMES UP:11 , 0.70
		TIMES UP:12 , 0.40
	ELSEIF A >= 5000
		E = 8
		TIMES UP:11 , 0.75
		TIMES UP:12 , 0.50
	ELSEIF A >= 3000
		E = 4
		TIMES UP:11 , 0.80
		TIMES UP:12 , 0.60
	ELSEIF A >= 2000
		E = 2
		TIMES UP:11 , 0.85
		TIMES UP:12 , 0.70
	ELSEIF A >= 1000
		E = 1
		TIMES UP:11 , 0.90
		TIMES UP:12 , 0.80
	ENDIF
	IF E
		PRINT 奉仕快楽経験+
		PRINTVL E
		EXP:21 += E
	ENDIF
ENDIF

