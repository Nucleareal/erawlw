;=============================================================================
;-------------------------------------------------
;売却・解放キャラの選択
;-------------------------------------------------
@CHARA_SALE

VARSET LOCAL, 0

$PRINT_LIST
DRAWLINE
PRINTFORML 誰を売却しますか？
DRAWLINE
REPEAT CHARANUM
	;主人は売れない(2重の意味で)
	SIF COUNT == MASTER
		CONTINUE
	
	CALL CHARA_SALE_CHECK, COUNT
	IF RESULT == 0
		PRINTFORML [-] - %NAME:COUNT% ×売却可能な条件を満たしていません
		CONTINUE
	ENDIF
	
	IF TALENT:COUNT:89
		PRINTFORML [-] - %NAME:COUNT% ×烙印が押されています
		CONTINUE
	ENDIF
		
	PRINTFORM [{COUNT}] - %NAME:COUNT% ○売却可能です
	
	IF ASSI == COUNT
		FONTSTYLE 1
		PRINT  (助手)
		FONTSTYLE 0
	ELSEIF ISASSI:COUNT == 1
		PRINT (元助手)
	ENDIF
	
	PRINTL
REND

DRAWLINE

PRINTLC [1000]戻る
PRINTL 

$INPUT_LOOP
INPUT

LOCAL:11 = RESULT

IF RESULT == 1000
	RETURN 0
ELSEIF RESULT <= 0 || RESULT >= CHARANUM
	GOTO INPUT_LOOP
ELSEIF TALENT:(LOCAL:11):89
	GOTO INPUT_LOOP
ELSE
	CALL CHARA_SALE_CHECK, LOCAL:11
	SIF RESULT == 0
		GOTO INPUT_LOOP
ENDIF

LOCAL = RESULT
CALL SALE_CONFIRM, LOCAL:11

RESTART

