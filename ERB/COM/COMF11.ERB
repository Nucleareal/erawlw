@COM11
TSTR:0 = バイブ
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM
;バイブ
LOSEBASE:0 += 30
LOSEBASE:1 += 100

;ABL:Ｖ感覚をみる
IF ABL:4 == 0
	SOURCE:1 = 80
ELSEIF ABL:4 == 1
	SOURCE:1 = 250
ELSEIF ABL:4 == 2
	SOURCE:1 = 600
ELSEIF ABL:4 == 3
	SOURCE:1 = 1000
ELSEIF ABL:4 == 4
	SOURCE:1 = 1300
ELSE
	SOURCE:1 = 1700
ENDIF

;EXP:Ｖ経験をみる
IF EXP:0 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 = 5500

	EXP:50 += 1
	PRINTFORML %EXPNAME:50%＋１
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 = 300
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:6 = 50
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:6 = 10
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:1 , 1.40
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:1 , 1.60
	SOURCE:6 = 0
ENDIF

;名器なら潤滑を上げる
IF TALENT:130
	PALAM:3 += 1000
ENDIF
C = PALAM:3

;PALAM:潤滑をみる
IF C < PALAMLV:1
	TIMES SOURCE:1 , 0.10
	SOURCE:6 += 1000
	TIMES SOURCE:6 , 3.00
ELSEIF C < PALAMLV:2
	TIMES SOURCE:1 , 0.40
	SOURCE:6 += 400
	TIMES SOURCE:6 , 1.00
ELSEIF C < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF C < PALAMLV:4
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:6 , 0.20
ELSEIF C >= PALAMLV:4
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:6 , 0.10
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:1 , 0.80
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:1 , 0.90
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:1 , 1.10
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:1 , 1.20
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES SOURCE:1 , 0.80
ELSEIF ABL:0 == 1
	TIMES SOURCE:1 , 0.90
ELSEIF ABL:0 == 2
	TIMES SOURCE:1 , 1.00
ELSEIF ABL:0 == 3
	TIMES SOURCE:1 , 1.10
ELSEIF ABL:0 == 4
	TIMES SOURCE:1 , 1.20
ELSE
	TIMES SOURCE:1 , 1.30
ENDIF

;小柄体形
SIF TALENT:100
	TIMES SOURCE:6 , 2.00
	
;∨敏感、鈍感をみる
IF TALENT:104
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:13 , 1.50
	TIMES SOURCE:14 , 1.50
ELSEIF TALENT:103
	TIMES SOURCE:6 , 0.60
	TIMES SOURCE:13 , 0.60
	TIMES SOURCE:14 , 0.60
ENDIF

EXP:0 += 1
PRINTL Ｖ経験＋１

;バイブの着脱
IF TEQUIP:1
	TEQUIP:1 = 0
ELSE
	TEQUIP:1 = 1
	TEQUIP:7 = 0
ENDIF
CALL DISPLAY_EQUIP_MESSAGE_COM_SLAVE, 1
CALL DISPLAY_EQUIP_MESSAGE_COM_ASSI, 1

RETURN 1




@EQUIP_COM11
;バイブ挿入中

PRINTL ＜バイブ挿入中＞

LOSEBASE:0 += 10
LOSEBASE:1 += 50

;ABL:Ｖ感覚をみる
IF ABL:4 == 0
	A = 40
ELSEIF ABL:4 == 1
	A = 120
ELSEIF ABL:4 == 2
	A = 250
ELSEIF ABL:4 == 3
	A = 450
ELSEIF ABL:4 == 4
	A = 600
ELSE
	A = 750
ENDIF

;EXP:Ｖ経験をみる
;処女はありえない
IF EXP:0 < EXPLV:2
	TIMES A , 0.60
	B = 150
ELSEIF EXP:0 < EXPLV:3
	TIMES A , 1.00
	B = 20
ELSEIF EXP:0 < EXPLV:4
	TIMES A , 1.20
	B = 0
ELSEIF EXP:0 < EXPLV:5
	TIMES A , 1.40
	B = 0
ELSE
	TIMES A , 1.60
	B = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES A , 0.10
	B += 400
	TIMES B , 3.00
	C += 1000
ELSEIF PALAM:3 < PALAMLV:2
	TIMES A , 0.40
	B += 150
	TIMES B , 1.00
	C += 200
ELSEIF PALAM:3 < PALAMLV:3
	TIMES A , 1.00
	TIMES B , 0.50
	C += 80
ELSEIF PALAM:3 < PALAMLV:4
	TIMES A , 1.40
	TIMES B , 0.20
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES A , 1.80
	TIMES B , 0.10
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:5 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:5 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES A , 1.10
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES A , 1.20
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES A , 0.80
ELSEIF ABL:0 == 1
	TIMES A , 0.90
ELSEIF ABL:0 == 2
	TIMES A , 1.00
ELSEIF ABL:0 == 3
	TIMES A , 1.10
ELSEIF ABL:0 == 4
	TIMES A , 1.20
ELSE
	TIMES A , 1.30
ENDIF

;小柄体形
SIF TALENT:100
	TIMES B , 2.00

;貞操観念
IF TALENT:100
	;処女はありえない
	TIMES C, 3.00
ENDIF

SOURCE:1 += A
SOURCE:6 += B

EXP:0 += 2
PRINTL Ｖ経験＋２

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:40 += 1
	
	SIF PLAYER != MASTER
		EXP:PLAYER:40 += 1
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:41 += 1
ENDIF

