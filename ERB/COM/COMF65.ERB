@COM65
;助手を犯す

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:1
	SIF S
		PRINT  + 
	A += ABL:1 * 2
	PRINTS ABLNAME:1
	PRINTV 'LV,ABL:1,'(,ABL:1 * 2,')
	S = 1
ENDIF
;ABL:奉仕精神
IF ABL:6
	SIF S
		PRINT  + 
	A += ABL:6 * 4
	PRINTS ABLNAME:6
	PRINTV 'LV,ABL:6,'(,ABL:6 * 4,')
	S = 1
ENDIF

IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 2
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 2,')
	S = 1
ENDIF


;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 2
	PRINTS PALAMNAME:5
	PRINTV 'LV,L,'(,L * 2,')
	S = 1
ENDIF

;恥じらい
IF TALENT:35
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:35
	PRINTV '(,1,')
	S = 1
ENDIF
;献身的
IF TALENT:63
	SIF S
		PRINT  + 
	A += 6
	PRINTS TALENTNAME:63
	PRINTV '(,6,')
	S = 1
ENDIF
;快感に素直
IF TALENT:71
	SIF S
		PRINT  + 
	A += 2
	PRINTS TALENTNAME:71
	PRINTV '(,2,')
	S = 1
ENDIF
;快感の否定
IF TALENT:71
	PRINT  - 
	A -= 2
	PRINTS TALENTNAME:71
	PRINTV '(,2,')
	S = 1
ENDIF
;恋慕
IF TALENT:85 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:85
	PRINTV '(,5,')
	S = 1
ENDIF

;助手が処女
IF EXP:PLAYER:0 == 0
	PRINT  - 
	A -= 15
	PRINT 助手が処女
	PRINTV '(,15,')
	S = 1
ENDIF

;合計を表示(40以上で実行)
PRINT  = 
PRINTV A

V = 40
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT

;実行できない
IF A < V
	CALL DISPLAY_KOJO_DENIAL_MESSAGE_COM, 65
	RETURN 0
ENDIF

;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = 助手を犯す
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM
;-------------------------------------------------
;射精ゲージチェック（ふたなりの場合）
;-------------------------------------------------
B = 0

;ABL:技巧をみる
IF ABL:2 == 0
	B = 50
ELSEIF ABL:2 == 1
	B = 300
ELSEIF ABL:2 == 2
	B = 800
ELSEIF ABL:2 == 3
	B = 1500
ELSEIF ABL:2 == 4
	B = 2000
ELSE
	B = 3200
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:奉仕精神をみる
IF ABL:6 == 0
	TIMES B , 0.50
ELSEIF ABL:6 == 1
	TIMES B , 0.80
ELSEIF ABL:6 == 2
	TIMES B , 1.20
ELSEIF ABL:6 == 3
	TIMES B , 1.50
ELSEIF ABL:6 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;助手のABL:V感覚をみる
IF ABL:ASSI:4 == 0
	TIMES B , 0.50
ELSEIF ABL:ASSI:4 == 1
	TIMES B , 0.80
ELSEIF ABL:ASSI:4 == 2
	TIMES B , 1.00
ELSEIF ABL:ASSI:4 == 3
	TIMES B , 1.20
ELSEIF ABL:ASSI:4 == 4
	TIMES B , 1.50
ELSE
	TIMES B , 2.00
ENDIF

;助手のEXP:Ｖ経験をみる
IF EXP:ASSI:0 < EXPLV:1
	TIMES B , 0.20
ELSEIF EXP:ASSI:0 < EXPLV:2
	TIMES B , 0.50
ELSEIF EXP:ASSI:0 < EXPLV:3
	TIMES B , 0.80
ELSEIF EXP:ASSI:0 < EXPLV:4
	TIMES B , 1.00
ELSEIF EXP:ASSI:0 < EXPLV:5
	TIMES B , 1.20
ELSE
	TIMES B , 1.40
ENDIF

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF TALENT:PLAYER:121 || TALENT:PLAYER:122
	BASE:PLAYER:2 += B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:1 += 40
LOSEBASE:1 += 220

SOURCE:13 = 1500
SOURCE:14 = 800

;ABL:奉仕精神をみる
IF ABL:6 == 0
	SOURCE:4 = 3200
	SOURCE:5 = 400
ELSEIF ABL:6 == 1
	SOURCE:4 = 3800
	SOURCE:5 = 800
ELSEIF ABL:6 == 2
	SOURCE:4 = 4600
	SOURCE:5 = 1500
ELSEIF ABL:6 == 3
	SOURCE:4 = 5400
	SOURCE:5 = 2300
ELSEIF ABL:6 == 4
	SOURCE:4 = 6200
	SOURCE:5 = 3500
ELSE
	SOURCE:4 = 7000
	SOURCE:5 = 5000
ENDIF


;ABL:技巧をみる
IF ABL:2 == 0
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:2 == 1
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:2 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:2 == 3
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSEIF ABL:2 == 4
	TIMES SOURCE:4 , 2.50
	TIMES SOURCE:5 , 2.50
ELSE
	TIMES SOURCE:4 , 4.00
	TIMES SOURCE:5 , 4.00
ENDIF

;助手が処女
IF EXP:PLAYER:0 == 0
	TIMES SOURCE:13 , 20.00
	TIMES SOURCE:14 , 3.00

	PRINTL ＜助手が処女＞

	EXP:50 += 1
	PRINTFORML %EXPNAME:50%＋１

	IF ABL:0 < 3
		PRINTFORML そして%ABLNAME:0%が3になった
		ABL:0 = 3
	ENDIF
ENDIF
EXP:PLAYER:0 += 1

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
S = BASE:PLAYER:2
EJAC = MAXBASE:PLAYER:2

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

IF E
;射精している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:11 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:11 == 1
		SOURCE:7 = 50
		TIMES SOURCE:5 , 2.50
		TIMES SOURCE:13, 1.60
	ELSEIF ABL:11 == 2
		SOURCE:7 = 150
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 1.00
	ELSEIF ABL:11 == 3
		SOURCE:7 = 300
		TIMES SOURCE:5 , 4.50
		TIMES SOURCE:13, 0.70
	ELSEIF ABL:11 == 4
		SOURCE:7 = 600
		TIMES SOURCE:5 , 6.00
		TIMES SOURCE:13, 0.40
	ELSE
		SOURCE:7 = 1200
		TIMES SOURCE:5 , 8.00
		TIMES SOURCE:13, 0.10
	ENDIF
ENDIF

;大量射精
IF E == 2
	TIMES SOURCE:7 , 2.00
	TIMES SOURCE:5 , 1.50

	EXP:PLAYER:3 += 2
	EXP:20 += 1
	PRINTL 大量射精
	PRINTL 精液経験＋１
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC*2
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;助手を犯すで射精させたフラグ
	TFLAG:7 = 2
;通常の射精
ELSEIF E == 1
	EXP:PLAYER:3 += 1
	PRINTL 射精
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;助手を犯すで射精させたフラグ
	TFLAG:7 = 1
ENDIF

IF TALENT:121 || TALENT:122
	;奴隷のＰ⇔調教者のＶの汚れが移動
	STAIN:2 |= STAIN:PLAYER:3
	STAIN:PLAYER:3 |= STAIN:2
ENDIF

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +10
	EXP:40 += 10
	SIF PLAYER != MASTER
		EXP:PLAYER:40 += 10
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +10
	EXP:41 += 10
ENDIF

TFLAG:100 = 1

;屈服刻印２に相当
TFLAG:200 = 2

RETURN 1

