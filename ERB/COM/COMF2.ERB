@COM2
TSTR:0 = アナル愛撫
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM

;アナル愛撫
LOSEBASE:0 += 20
LOSEBASE:1 += 100

SOURCE:12 = 850
SOURCE:14 = 400

;ABL:Ａ感覚をみる
IF ABL:5 == 0
	SOURCE:2 = 20
	SOURCE:13 = 300
ELSEIF ABL:5 == 1
	SOURCE:2 = 75
	SOURCE:13 = 350
ELSEIF ABL:5 == 2
	SOURCE:2 = 300
	SOURCE:13 = 400
ELSEIF ABL:5 == 3
	SOURCE:2 = 700
	SOURCE:13 = 650
ELSEIF ABL:5 == 4
	SOURCE:2 = 1100
	SOURCE:13 = 1000
ELSE
	SOURCE:2 = 1500
	SOURCE:13 = 1500
ENDIF

;EXP:Ａ経験をみる
IF EXP:1 < EXPLV:1
	TIMES SOURCE:2 , 0.20
	TIMES SOURCE:13, 0.20
	SOURCE:6 = 500
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:2 , 0.50
	TIMES SOURCE:13, 0.50
	SOURCE:6 = 400
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:13, 1.00
	SOURCE:6 = 300
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:2 , 1.20
	TIMES SOURCE:13, 1.20
	SOURCE:6 = 200
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:2 , 1.60
	TIMES SOURCE:13, 1.60
	SOURCE:6 = 100
ELSE
	TIMES SOURCE:2 , 1.80
	TIMES SOURCE:13, 1.80
	SOURCE:6 = 50
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:2 , 0.10
	TIMES SOURCE:13, 0.10
	TIMES SOURCE:6 , 3.00
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:2 , 0.20
	TIMES SOURCE:13, 0.20
	TIMES SOURCE:6 , 2.00
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:2 , 0.60
	TIMES SOURCE:13, 0.60
	TIMES SOURCE:6 , 1.00
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:13, 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES SOURCE:2 , 2.00
	TIMES SOURCE:13, 2.00
	TIMES SOURCE:6 , 0.10
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:2 , 0.30
	TIMES SOURCE:13, 0.30
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:2 , 0.60
	TIMES SOURCE:13, 0.60
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:13, 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:2 , 1.30
	TIMES SOURCE:13, 1.30
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:2 , 1.60
	TIMES SOURCE:13, 1.60
ENDIF


;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:105
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:13 , 1.50
	TIMES SOURCE:14 , 1.50
ELSEIF TALENT:106
	TIMES SOURCE:6 , 0.60
	TIMES SOURCE:13 , 0.60
	TIMES SOURCE:14 , 0.60
ENDIF

;処女で貞操観念
IF EXP:0 == 0 && TALENT:30
	SOURCE:13 /= 3
ENDIF

EXP:1 += 1
PRINTL Ａ経験＋１

;奴隷のＶ⇔調教者のＰの汚れが移動
STAIN:4 |= STAIN:PLAYER:1
STAIN:PLAYER:1 |= STAIN:4

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +2
	EXP:40 += 2
	
	SIF PLAYER != MASTER
		EXP:PLAYER:40 += 1
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +5
	EXP:41 += 5
ENDIF


RETURN 1

