@COM92
;授乳手コキ

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:1
	SIF S
		PRINT  + 
	A += ABL:1 * 1
	PRINTS ABLNAME:1
	PRINTV 'LV,ABL:1,'(,ABL:1 * 1,')
	S = 1
ENDIF
;ABL:奉仕精神
IF ABL:6
	SIF S
		PRINT  + 
	A += ABL:6 * 4
	PRINTS ABLNAME:6
	PRINTV 'LV,ABL:6,'(,ABL:6 * 4,')
	S = 1
ENDIF
;ABL:精液中毒
IF ABL:11
	SIF S
		PRINT  + 
	A += ABL:11 * 1
	PRINTS ABLNAME:11
	PRINTV 'LV,ABL:11,'(,ABL:11 * 1,')
	S = 1
ENDIF

IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 1
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 1,')
	S = 1
ENDIF


;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 1
	PRINTS PALAMNAME:5
	PRINTV 'LV,L,'(,L * 1,')
	S = 1
ENDIF

;恥じらい
IF TALENT:35
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:35
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭鈍感
IF TALENT:61
	SIF S
		PRINT  + 
	A += 1
	PRINTS TALENTNAME:61
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭敏感
IF TALENT:62
	PRINT  - 
	A -= 4
	PRINTS TALENTNAME:62
	PRINTV '(,4,')
	S = 1
ENDIF
;献身的
IF TALENT:63
	SIF S
		PRINT  + 
	A += 6
	PRINTS TALENTNAME:63
	PRINTV '(,6,')
	S = 1
ENDIF
;快感の否定
IF TALENT:71
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:71
	PRINTV '(,1,')
	S = 1
ENDIF
;男嫌い
IF TALENT:82 && TALENT:PLAYER:122
	PRINT  - 
	A -= 8
	PRINTS TALENTNAME:82
	PRINTV '(,8,')
	S = 8
ENDIF
;恋慕
IF TALENT:85 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:85
	PRINTV '(,5,')
	S = 5
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:121
	SIF S
		PRINT  + 
	A += 4
	PRINTS TALENTNAME:121
	PRINTV '(,4,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 6
SIF STAIN:PLAYER:2 & 16
	Y += 3

SIF TALENT:61
	Y /= 2
SIF TALENT:62
	Y *= 3

;手淫では汚れがそれほど影響しない
Y /= 2

;汚れあり
IF Y
	PRINT  - 
	A -= Y
	;汚臭鈍感
	IF TALENT:61
		PRINT 汚れあり、
		PRINTS TALENTNAME:61
	ELSEIF TALENT:62
		PRINT 汚れあり、
		PRINTS TALENTNAME:62
	ELSE
		PRINT 汚れあり
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF


;合計を表示(24以上で実行)
PRINT  = 
PRINTV A

V = 24
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT

;実行できない
IF A < V
	CALL DISPLAY_KOJO_DENIAL_MESSAGE_COM, 92
	RETURN 0
ENDIF

;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = 授乳手コキ
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
B = 0

;ABL:技巧をみる
IF ABL:2 == 0
	B = 450
ELSEIF ABL:2 == 1
	B = 1000
ELSEIF ABL:2 == 2
	B = 1600
ELSEIF ABL:2 == 3
	B = 2200
ELSEIF ABL:2 == 4
	B = 2700
ELSE
	B = 3200
ENDIF

;調教者が小柄体型で対象がそうでないならボーナス
SIF TALENT:PLAYER:100 && TALENT:TARGET:100 == 0
	TIMES B , 1.50

;ABL:従順をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:奉仕精神をみる
IF ABL:6 == 0
	TIMES B , 0.50
ELSEIF ABL:6 == 1
	TIMES B , 0.80
ELSEIF ABL:6 == 2
	TIMES B , 1.20
ELSEIF ABL:6 == 3
	TIMES B , 1.50
ELSEIF ABL:6 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF TALENT:PLAYER:121 || TALENT:PLAYER:122
	BASE:PLAYER:2 += B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
;体力, 気力の減少
LOSEBASE:0 += 10
LOSEBASE:1 += 150

;情愛
SOURCE:3 = 200
;欲情
SOURCE:11 = 100
;露出
SOURCE:12 = 100
;屈従
SOURCE:13 = 500
;逸脱
SOURCE:14 = 300
;反感
SOURCE:15 = 50

;調教者が小柄体型で対象がそうでないならボーナス
SIF TALENT:PLAYER:100 && TALENT:TARGET:100 == 0
	TIMES SOURCE:3 , 4.00

;上のほうで計算した汚れデータ
SOURCE:8 = Y * 12 + 60

;ABL:奉仕精神をみる
SELECTCASE ABL:6
	CASE 0
		TIMES SOURCE:3 , 0.50
		TIMES SOURCE:11 , 0.50
		TIMES SOURCE:12 , 0.50
		TIMES SOURCE:13 , 0.50
		TIMES SOURCE:14 , 2.00
		TIMES SOURCE:15 , 4.00
	CASE 1
		TIMES SOURCE:3 , 1.00
		TIMES SOURCE:11 , 1.00
		TIMES SOURCE:12 , 1.00
		TIMES SOURCE:13 , 1.00
		TIMES SOURCE:14 , 1.25
		TIMES SOURCE:15 , 1.50
	CASE 2
		TIMES SOURCE:3 , 1.25
		TIMES SOURCE:11 , 1.25
		TIMES SOURCE:12 , 1.25
		TIMES SOURCE:13 , 1.25
		TIMES SOURCE:14 , 1.00
		TIMES SOURCE:15 , 1.00
	CASE 3
		TIMES SOURCE:3 , 1.50
		TIMES SOURCE:11 , 1.50
		TIMES SOURCE:12 , 1.50
		TIMES SOURCE:13 , 1.50
		TIMES SOURCE:14 , 0.80
		TIMES SOURCE:15 , 0.75
	CASE 4
		TIMES SOURCE:3 , 1.75
		TIMES SOURCE:11 , 1.75
		TIMES SOURCE:12 , 1.75
		TIMES SOURCE:13 , 1.75
		TIMES SOURCE:14 , 0.75
		TIMES SOURCE:15 , 0.55
	CASE 5
		TIMES SOURCE:3 , 2.00
		TIMES SOURCE:11 , 2.00
		TIMES SOURCE:12 , 2.00
		TIMES SOURCE:13 , 2.00
		TIMES SOURCE:14 , 0.50
		TIMES SOURCE:15 , 0.25
ENDSELECT

IF ABL:6 == 0
	SOURCE:4 = 250
	SOURCE:5 = 50
	TIMES SOURCE:8 , 4.00
ELSEIF ABL:6 == 1
	SOURCE:4 = 300
	SOURCE:5 = 100
	TIMES SOURCE:8 , 2.50
ELSEIF ABL:6 == 2
	SOURCE:4 = 350
	SOURCE:5 = 200
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:6 == 3
	SOURCE:4 = 400
	SOURCE:5 = 300
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:6 == 4
	SOURCE:4 = 450
	SOURCE:5 = 500
	TIMES SOURCE:8 , 0.50
ELSE
	SOURCE:4 = 500
	SOURCE:5 = 750
	TIMES SOURCE:8 , 0.10
ENDIF


;ABL:技巧をみる
IF ABL:2 == 0
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:2 == 1
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:2 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:2 == 3
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:2 == 4
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSE
	TIMES SOURCE:4 , 2.00
	TIMES SOURCE:5 , 2.00
ENDIF

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
S = BASE:PLAYER:2
EJAC = MAXBASE:PLAYER:2

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

IF E
;射精している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:11 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:11 == 1
		SOURCE:7 = 200
		TIMES SOURCE:5 , 2.50
		TIMES SOURCE:13, 1.60
	ELSEIF ABL:11 == 2
		SOURCE:7 = 600
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 1.00
	ELSEIF ABL:11 == 3
		SOURCE:7 = 1500
		TIMES SOURCE:5 , 4.00
		TIMES SOURCE:13, 0.70
	ELSEIF ABL:11 == 4
		SOURCE:7 = 3000
		TIMES SOURCE:5 , 5.00
		TIMES SOURCE:13, 0.40
	ELSE
		SOURCE:7 = 6000
		TIMES SOURCE:5 , 6.00
		TIMES SOURCE:13, 0.10
	ENDIF
ENDIF

;大量射精
IF E == 2
	TIMES SOURCE:7 , 2.00
	TIMES SOURCE:5 , 1.50

	EXP:PLAYER:3 += 2
	EXP:20 += 3
	PRINTL 大量射精
	PRINTL 精液経験＋３
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC*2
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;手で射精させたフラグ
	TFLAG:1 = 2
;通常の射精
ELSEIF E == 1
	EXP:PLAYER:3 += 1
	EXP:20 += 1
	PRINTL 射精
	PRINTL 精液経験＋１

	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;手で射精させたフラグ
	TFLAG:1 = 1
ENDIF

;奴隷の指⇔調教者のＰの汚れが移動
IF TALENT:PLAYER:121 || TALENT:PLAYER:122
	STAIN:1 |= STAIN:PLAYER:2
	STAIN:PLAYER:2 |= STAIN:1
ELSE
	STAIN:1 |= STAIN:PLAYER:3
	STAIN:PLAYER:3 |= STAIN:1
ENDIF

;奴隷の胸⇔調教者の口の汚れが移動
STAIN:5 |= STAIN:PLAYER:0
STAIN:PLAYER:0 |= STAIN:5

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +4
	EXP:40 += 4
	SIF PLAYER != MASTER
		EXP:PLAYER:40 += 1
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +4
	EXP:41 += 4
ENDIF

TFLAG:100 = 1

;調教者がふたなり
IF TALENT:PLAYER:121
	SOURCE:13 /= 2
ENDIF

RETURN 1

