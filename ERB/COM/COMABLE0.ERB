@COM_ABLE1
;クンニ実行判定

VARSET LOCAL, 0

;性器が汚れているとMASTERの場合クンニはできない（愛液のよごれはＯＫ）
SIF STAIN:3 >= 2 && ASSIPLAY == 0
	RETURN 0

;助手プレイ、かつ性器が汚れている
IF ASSIPLAY && STAIN:3 >= 2

	;従順3以下かつ汚臭敏感の場合不可
	SIF ABL:ASSI:0 <= 3 && TALENT:ASSI:62
		RETURN 0
	
	;従順4以下かつ汚臭鈍感でない場合
	IF ABL:ASSI:0 <= 4 && TALENT:ASSI:61 == 0
	
		;レズっ気3が必要
		LOCAL = 3
		;生理有効かつ生理中ならレズっ気5が必要
		IF MENSTRUATION_AVAILABLE && MENSTRUATION_CHECK(TARGET) > 0
			LOCAL = 5
		ENDIF
	
		;レズっ気3が必要
		SIF ABL:ASSI:9 < LOCAL
			RETURN 0
	ENDIF
ENDIF

;オトコだとダメ
SIF TALENT:122
	RETURN 0

RETURN 1

