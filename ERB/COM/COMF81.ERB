@COM81
TSTR:0 = ローターＡ挿入
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM
;アナルバイブ
LOSEBASE:0 += 45
LOSEBASE:1 += 100

;ABL:Ａ感覚をみる
SELECTCASE ABL:5
	CASE 0
		SOURCE:2 = 40
		SOURCE:13 = 150
	CASE 1
		SOURCE:2 = 120
		SOURCE:13 = 400
	CASE 2
		SOURCE:2 = 300
		SOURCE:13 = 700
	CASE 3
		SOURCE:2 = 500
		SOURCE:13 = 900
	CASE 4
		SOURCE:2 = 800
		SOURCE:13 = 1200
	CASE 5
		SOURCE:2 = 1300
		SOURCE:13 = 1500
ENDSELECT

;EXP:Ａ経験をみる
IF EXP:1 < EXPLV:1
	TIMES SOURCE:2 , 0.50
	SOURCE:6 = 2000
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:2 , 1.00
	SOURCE:6 = 300
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:2 , 1.10
	SOURCE:6 = 50
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:2 , 1.10
	SOURCE:6 = 10
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:2 , 1.20
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:2 , 1.30
	SOURCE:6 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:6 += 800
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:6 += 500
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:6 += 300
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	SOURCE:6 += 120
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES SOURCE:2 , 1.80
	SOURCE:6 += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:2 , 0.80
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:2 , 0.90
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:2 , 1.10
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:2 , 1.20
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES SOURCE:2 , 0.80
ELSEIF ABL:0 == 1
	TIMES SOURCE:2 , 0.90
ELSEIF ABL:0 == 2
	TIMES SOURCE:2 , 1.00
ELSEIF ABL:0 == 3
	TIMES SOURCE:2 , 1.10
ELSEIF ABL:0 == 4
	TIMES SOURCE:2 , 1.20
ELSE
	TIMES SOURCE:2 , 1.30
ENDIF

;小柄体形
SIF TALENT:100
	TIMES SOURCE:6 , 1.25

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:106
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:13 , 1.50
	TIMES SOURCE:14 , 1.50
ELSEIF TALENT:105
	TIMES SOURCE:6 , 0.60
	TIMES SOURCE:13 , 0.60
	TIMES SOURCE:14 , 0.60
ENDIF

IF EXP:0 == 0 && TALENT:30
	SOURCE:13 /= 3
ENDIF

EXP:1 += 1
PRINTFORML %EXPNAME:1%＋１

;アナルバイブの着脱
IF TEQUIP:91
	TEQUIP:91 = 0
ELSE
	TEQUIP:91 = 1
	TEQUIP:90 = 0
ENDIF
CALL DISPLAY_EQUIP_MESSAGE_COM_SLAVE, 81
CALL DISPLAY_EQUIP_MESSAGE_COM_ASSI, 81

RETURN 1



@EQUIP_COM81
;アナルバイブ

PRINTL ＜ローターＡ挿入中＞

LOSEBASE:0 += 15
LOSEBASE:1 += 40

;ABL:Ａ感覚をみる
IF ABL:5 == 0
	A = 40
	B = 150
ELSEIF ABL:5 == 1
	A = 120
	B = 400
ELSEIF ABL:5 == 2
	A = 300
	B = 700
ELSEIF ABL:5 == 3
	A = 500
	B = 900
ELSEIF ABL:5 == 4
	A = 900
	B = 1300
ELSE
	A = 1300
	B = 1700
ENDIF

;EXP:Ａ経験をみる
IF EXP:1 < EXPLV:1
	TIMES A , 0.50
	C = 1000
ELSEIF EXP:1 < EXPLV:2
	TIMES A , 1.00
	C = 150
ELSEIF EXP:1 < EXPLV:3
	TIMES A , 1.10
	C = 25
ELSEIF EXP:1 < EXPLV:4
	TIMES A , 1.20
	C = 5
ELSEIF EXP:1 < EXPLV:5
	TIMES A , 1.40
	C = 0
ELSE
	TIMES A , 1.60
	C = 0
ENDIF

;Ａ名器なら潤滑を上げる
IF TALENT:131
	PALAM:3 += 1000
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES A , 0.40
	C += 400
ELSEIF PALAM:3 < PALAMLV:2
	TIMES A , 0.80
	C += 250
ELSEIF PALAM:3 < PALAMLV:3
	TIMES A , 1.00
	C += 150
ELSEIF PALAM:3 < PALAMLV:4
	TIMES A , 1.40
	C += 60
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES A , 1.80
	C += 50
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:5 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:5 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES A , 1.10
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES A , 1.20
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES A , 0.80
ELSEIF ABL:0 == 1
	TIMES A , 0.90
ELSEIF ABL:0 == 2
	TIMES A , 1.00
ELSEIF ABL:0 == 3
	TIMES A , 1.10
ELSEIF ABL:0 == 4
	TIMES A , 1.20
ELSE
	TIMES A , 1.30
ENDIF

;小柄体形
SIF TALENT:100
	TIMES C , 1.25

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:106
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:13 , 1.50
	TIMES SOURCE:14 , 1.50
ELSEIF TALENT:105
	TIMES SOURCE:6 , 0.60
	TIMES SOURCE:13 , 0.60
	TIMES SOURCE:14 , 0.60
ENDIF

SOURCE:2 += A
SOURCE:13 += B
SOURCE:6 += C

;処女で貞操観念
IF EXP:0 == 0 && TALENT:30
	SOURCE:13 /= 3
ENDIF

EXP:1 += 1
PRINTL Ａ経験＋１

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:40 += 1
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:41 += 1
ENDIF

RETURN 1
