@COM37
;イラマチオ

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:1
	SIF S
		PRINT  + 
	A += ABL:1 * 2
	PRINTS ABLNAME:1
	PRINTV 'LV,ABL:1,'(,ABL:1 * 1,')
	S = 1
ENDIF
;ABL:奉仕精神
IF ABL:6
	SIF S
		PRINT  + 
	A += ABL:6 * 3
	PRINTS ABLNAME:6
	PRINTV 'LV,ABL:6,'(,ABL:6 * 4,')
	S = 1
ENDIF
;ABL:精液中毒
IF ABL:11
	SIF S
		PRINT  + 
	A += ABL:11 * 4
	PRINTS ABLNAME:11
	PRINTV 'LV,ABL:11,'(,ABL:11 * 3,')
	S = 1
ENDIF

IF MARK:2
	SIF S
		PRINT  + 
	A += MARK:2 * 2
	PRINTS MARKNAME:2
	PRINTV 'LV,MARK:2,'(,MARK:2 * 2,')
	S = 1
ENDIF


;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 1
	PRINTS PALAMNAME:5
	PRINTV 'LV,L,'(,L * 1,')
	S = 1
ENDIF

;恥じらい
IF TALENT:35
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:35
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭鈍感
IF TALENT:61
	SIF S
		PRINT  + 
	A += 1
	PRINTS TALENTNAME:61
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭敏感
IF TALENT:62
	PRINT  - 
	A -= 5
	PRINTS TALENTNAME:62
	PRINTV '(,5,')
	S = 1
ENDIF
;献身的
IF TALENT:63
	SIF S
		PRINT  + 
	A += 6
	PRINTS TALENTNAME:63
	PRINTV '(,6,')
	S = 1
ENDIF
;快感の否定
IF TALENT:71
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:71
	PRINTV '(,1,')
	S = 1
ENDIF
;男嫌い
IF TALENT:82 && TALENT:PLAYER:122
	PRINT  - 
	A -= 15
	PRINTS TALENTNAME:82
	PRINTV '(,15,')
	S = 1
ENDIF
;恋慕
IF TALENT:85 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:85
	PRINTV '(,5,')
	S = 1
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:121
	SIF S
		PRINT  + 
	A += 6
	PRINTS TALENTNAME:121
	PRINTV '(,6,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 2
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 4
;アナルの汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 8
SIF STAIN:PLAYER:2 & 16
	Y += 16

SIF TALENT:61
	Y /= 2
SIF TALENT:62
	Y *= 3



;汚れあり
IF Y
	PRINT  - 
	A -= Y
	;汚臭鈍感
	IF TALENT:61
		PRINT 汚れあり、
		PRINTS TALENTNAME:61
	ELSEIF TALENT:62
		PRINT 汚れあり、
		PRINTS TALENTNAME:62
	ELSE
		PRINT 汚れあり
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(48以上で実行)
PRINT  = 
PRINTV A

V = 48
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT

;実行できない
IF A < V
	CALL DISPLAY_KOJO_DENIAL_MESSAGE_COM, 37
	RETURN 0
ENDIF

;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = イラマチオ
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
B = 0

;ABL:技巧
SELECTCASE ABL:2
	CASE 0
		B = 1000
	CASE 1
		B = 1500
	CASE 2
		B = 2300
	CASE 3
		B = 3200
	CASE 4
		B = 4500
	CASEELSE
		B = 6000
ENDSELECT

;ABL:従順
SELECTCASE ABL:0
	CASE 0
		TIMES B , 0.75
	CASE 1
		TIMES B , 0.80
	CASE 2
		TIMES B , 0.90
	CASE 3
		TIMES B , 1.05
	CASE 4
		TIMES B , 1.25
	CASE 5
		TIMES B , 1.50
ENDSELECT

;ABL:奉仕精神
SELECTCASE ABL:6
	CASE 0
		TIMES B , 0.25
	CASE 1
		TIMES B , 0.50
	CASE 2
		TIMES B , 1.00
	CASE 3
		TIMES B , 1.50
	CASE 4
		TIMES B , 2.00
	CASE 5
		TIMES B , 3.00
ENDSELECT

;ABL:精液中毒
SELECTCASE ABL:11
	CASE 0
		TIMES B , 0.75
	CASE 1
		TIMES B , 1.00
	CASE 2
		TIMES B , 1.25
	CASE 3
		TIMES B , 1.75
	CASE 4
		TIMES B , 2.50
	CASE 5
		TIMES B , 4.00
ENDSELECT

;TALENT:舌使いをみる
SIF TALENT:52
	TIMES B , 1.25

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF TALENT:PLAYER:121 || TALENT:PLAYER:122
	BASE:PLAYER:2 += B


;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 30
LOSEBASE:1 += 200

SOURCE:13 = 3000
SOURCE:14 = 1000

;上のほうで計算した汚れデータ
SOURCE:8 = Y * 50 + 200

;ABL:奉仕精神をみる
SELECTCASE ABL:6
	CASE 0
		SOURCE:4 = 600
		SOURCE:5 = 300
		TIMES SOURCE:8 , 6.00
	CASE 1
		SOURCE:4 = 650
		SOURCE:5 = 450
		TIMES SOURCE:8 , 3.00
	CASE 2
		SOURCE:4 = 700
		SOURCE:5 = 700
		TIMES SOURCE:8 , 1.50
	CASE 3
		SOURCE:4 = 750
		SOURCE:5 = 1200
		TIMES SOURCE:8 , 1.00
	CASE 4
		SOURCE:4 = 800
		SOURCE:5 = 1800
		TIMES SOURCE:8 , 0.25
	CASE 5
		SOURCE:4 = 900
		SOURCE:5 = 2500
		TIMES SOURCE:8 , 0.05
ENDSELECT

;ABL:技巧をみる
IF ABL:2 == 0
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:2 == 1
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:2 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:2 == 3
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:2 == 4
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSE
	TIMES SOURCE:4 , 2.00
	TIMES SOURCE:5 , 2.00
ENDIF

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
S = BASE:PLAYER:2
EJAC = MAXBASE:PLAYER:2

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

IF E
;射精している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:11 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 4.00
	ELSEIF ABL:11 == 1
		SOURCE:7 = 500
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 3.00
	ELSEIF ABL:11 == 2
		SOURCE:7 = 1200
		TIMES SOURCE:5 , 4.00
		TIMES SOURCE:13, 2.50
	ELSEIF ABL:11 == 3
		SOURCE:7 = 3000
		TIMES SOURCE:5 , 6.00
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:11 == 4
		SOURCE:7 = 6000
		TIMES SOURCE:5 , 9.00
		TIMES SOURCE:13, 1.50
	ELSE
		SOURCE:7 = 12000
		TIMES SOURCE:5 , 15.00
		TIMES SOURCE:13, 1.00
	ENDIF
ENDIF

;大量射精
IF E == 2
	TIMES SOURCE:7 , 2.00
	TIMES SOURCE:5 , 1.50

	EXP:PLAYER:3 += 2
	EXP:20 += 9
	PRINTL 大量射精
	PRINTL 精液経験＋９
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC*2
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;口で射精させたフラグ
	TFLAG:0 = 2
;通常の射精
ELSEIF E == 1
	EXP:PLAYER:3 += 1
	EXP:20 += 3
	PRINTL 射精
	PRINTL 精液経験＋３
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;口で射精させたフラグ
	TFLAG:0 = 1
ENDIF

PRINTFORM %EXPNAME:22%＋１
EXP:22 += 1

;奴隷の口⇔調教者のＰの汚れが移動
STAIN:0 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:0

;舐め取る
IF ABL:2 >= 2 && ABL:6 >= 2
	STAIN:PLAYER:2 = 2
ENDIF

;奉仕精神ＬＶ２以上、技巧ＬＶ２以上なら汚れをなめ取る
IF ABL:6 >= 2 && ABL:2 >= 2
	STAIN:PLAYER:2 = 2
ENDIF

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +7
	EXP:40 += 7
	SIF PLAYER != MASTER
		EXP:PLAYER:40 += 1
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +7
	EXP:41 += 7
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:121
	SOURCE:13 /= 2
ENDIF

TFLAG:100 = 1

;屈服刻印３に相当
TFLAG:200 = 3

RETURN 1


