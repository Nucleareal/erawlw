@COM201
TSTR:0 = Ａプラグ(狐尻尾)
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM
;アナルバイブ
LOSEBASE:0 += 50
LOSEBASE:1 += 100

;このコマンド単体では効果を持たない
;Ａプラグを引っ張るコマンドによる

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	SOURCE:6 += 800
ELSEIF PALAM:3 < PALAMLV:2
	SOURCE:6 += 500
ELSEIF PALAM:3 < PALAMLV:3
	SOURCE:6 += 300
ELSEIF PALAM:3 < PALAMLV:4
	SOURCE:6 += 120
ELSEIF PALAM:3 >= PALAMLV:4
	SOURCE:6 += 100
ENDIF

;小柄体形
SIF TALENT:100
	TIMES SOURCE:6 , 2.00

;アナルプラグの着脱
IF TEQUIP:6
	TEQUIP:6 = 0
ELSE
	TEQUIP:6 = 1
	TEQUIP:3 = 0
	TEQUIP:5 = 0
ENDIF
CALL DISPLAY_EQUIP_MESSAGE_COM_SLAVE, 201
CALL DISPLAY_EQUIP_MESSAGE_COM_ASSI, 201

RETURN 1



@EQUIP_COM6

RETURN 1
