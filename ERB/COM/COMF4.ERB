@COM4
TSTR:0 = フェラする
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM
;フェラする
LOSEBASE:0 += 5
LOSEBASE:1 += 50

SOURCE:12 = 220
SOURCE:14 = 50

;ABL:Ｃ感覚をみる
IF ABL:3 == 0
	SOURCE:0 = 800
ELSEIF ABL:3 == 1
	SOURCE:0 = 1400
ELSEIF ABL:3 == 2
	SOURCE:0 = 2000
ELSEIF ABL:3 == 3
	SOURCE:0 = 2500
ELSEIF ABL:3 == 4
	SOURCE:0 = 2900
ELSE
	SOURCE:0 = 3200
ENDIF

;調教者のTALENT:舌使いをみる
IF TALENT:PLAYER:52
	TIMES SOURCE:0 , 2.00
	SOURCE:16 += SOURCE:0/20
ENDIF

;奴隷のＰ⇔調教者の口の汚れが移動
STAIN:2 |= STAIN:PLAYER:0
STAIN:PLAYER:0 |= STAIN:2

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +3
	EXP:40 += 3
	
	SIF PLAYER != MASTER
		EXP:PLAYER:40 += 1
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +3
	EXP:41 += 3
ENDIF

RETURN 1

