@COM34
;騎乗位

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:1
	SIF S
		PRINT  + 
	A += ABL:1 * 3
	PRINTS ABLNAME:1
	PRINTV 'LV,ABL:1,'(,ABL:1 * 3,')
	S = 1
ENDIF
;ABL:Ｖ感覚
IF ABL:4
	SIF S
		PRINT  + 
	A += ABL:4 * 2
	PRINTS ABLNAME:4
	PRINTV 'LV,ABL:4,'(,ABL:4 * 2,')
	S = 1
ENDIF
;ABL:奉仕精神
IF ABL:6
	SIF S
		PRINT  + 
	A += ABL:6 * 4
	PRINTS ABLNAME:6
	PRINTV 'LV,ABL:6,'(,ABL:6 * 4,')
	S = 1
ENDIF
;ABL:精液中毒
IF ABL:11
	SIF S
		PRINT  + 
	A += ABL:11 * 1
	PRINTS ABLNAME:11
	PRINTV 'LV,ABL:11,'(,ABL:11 * 1,')
	S = 1
ENDIF

IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 3
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 3,')
	S = 1
ENDIF

;PALAM:潤滑（の不足）
IF PALAM:3 < PALAMLV:3
	PRINT  - 
	A -= 5
	PRINT 潤滑不足
	PRINTV '(,5,')
	S = 1
ENDIF
;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 3
	PRINTS PALAMNAME:5
	PRINTV 'LV,L,'(,L * 3,')
	S = 1
ENDIF

;恥じらい
IF TALENT:35
	PRINT  - 
	A -= 2
	PRINTS TALENTNAME:35
	PRINTV '(,2,')
	S = 1
ENDIF
;快感の否定
IF TALENT:71
	PRINT  - 
	A -= 5
	PRINTS TALENTNAME:71
	PRINTV '(,5,')
	S = 1
ENDIF
;男嫌い
IF TALENT:82 && TALENT:PLAYER:122
	PRINT  - 
	A -= 12
	PRINTS TALENTNAME:82
	PRINTV '(,12,')
	S = 1
ENDIF
;恋慕
IF TALENT:85 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:85
	PRINTV '(,5,')
	S = 5
ENDIF


;処女
IF TALENT:0
	PRINT  - 
	A -= 20
	PRINTS TALENTNAME:0
	PRINTV '(,20,')
	S = 1
ELSEIF EXP:0 < EXPLV:2
	PRINT  - 
	A -= 5
	PRINTS EXPNAME:0
	PRINT 不足
	PRINTV '(,5,')
	S = 1
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:121
	SIF S
		PRINT  + 
	A += 4
	PRINTS TALENTNAME:121
	PRINTV '(,4,')
	S = 1
ENDIF

;媚薬
IF TEQUIP:21
	SIF S
		PRINT  + 
	A += 6
	PRINTS ITEMNAME:21
	PRINTV '(,6,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2

;騎乗位では汚れの影響が少なめ
Y /= 3

;汚れあり
IF Y
	PRINT  - 
	A -= Y
	;汚臭鈍感
	IF TALENT:61
		PRINT 汚れあり、
		PRINTS TALENTNAME:61
	ELSEIF TALENT:62
		PRINT 汚れあり、
		PRINTS TALENTNAME:62
	ELSE
		PRINT 汚れあり
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(24以上で実行)
PRINT  = 
PRINTV A

V = 24
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT

;実行できない
IF A < V
	CALL DISPLAY_KOJO_DENIAL_MESSAGE_COM, 34
	RETURN 0
ENDIF

;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = 騎乗位
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
B = 0

;ABL:技巧をみる
IF ABL:2 == 0
	B = 1000
ELSEIF ABL:2 == 1
	B = 1300
ELSEIF ABL:2 == 2
	B = 1700
ELSEIF ABL:2 == 3
	B = 2200
ELSEIF ABL:2 == 4
	B = 3000
ELSE
	B = 4500
ENDIF

SIF TALENT:130
	PALAM:3 += 1000
C = PALAM:3

;PALAM:潤滑
IF C < PALAMLV:1
	TIMES B , 0.20
ELSEIF C < PALAMLV:2
	TIMES B , 0.50
ELSEIF C < PALAMLV:3
	TIMES B , 1.00
ELSEIF C < PALAMLV:4
	TIMES B , 1.20
ELSEIF C < PALAMLV:5
	TIMES B , 1.50
ELSE
	TIMES B , 2.00
ENDIF

;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	TIMES B , 0.60
ELSEIF PALAM:5 < PALAMLV:2
	TIMES B , 0.80
ELSEIF PALAM:5 < PALAMLV:3
	TIMES B , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES B , 1.20
ELSEIF PALAM:5 < PALAMLV:5
	TIMES B , 1.40
ELSE
	TIMES B , 1.70
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:欲望をみる
IF ABL:1 == 0
	TIMES B , 1.00
ELSEIF ABL:1 == 1
	TIMES B , 1.10
ELSEIF ABL:1 == 2
	TIMES B , 1.20
ELSEIF ABL:1 == 3
	TIMES B , 1.30
ELSEIF ABL:1 == 4
	TIMES B , 1.40
ELSE
	TIMES B , 1.50
ENDIF

;ABL:奉仕精神をみる
IF ABL:6 == 0
	TIMES B , 0.30
ELSEIF ABL:6 == 1
	TIMES B , 0.70
ELSEIF ABL:6 == 2
	TIMES B , 1.00
ELSEIF ABL:6 == 3
	TIMES B , 1.20
ELSEIF ABL:6 == 4
	TIMES B , 1.50
ELSE
	TIMES B , 1.80
ENDIF

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;EXP:Ｖ経験をみる


;騎乗位特有の[Ｖ経験が多くなると快感が下がる]を名器ならスキップ
IF TALENT:130 == 0
	IF EXP:0 < EXPLV:1
		TIMES B , 1.50

		IF ASSIPLAY && TALENT:PLAYER:122 == 0
			EXP:50 += 3
			PRINTFORML %EXPNAME:50%＋３
		ELSE
			EXP:50 += 2
			PRINTFORML %EXPNAME:50%＋２
		ENDIF
	ELSEIF EXP:0 < EXPLV:2
		TIMES B , 1.00
	ELSEIF EXP:0 < EXPLV:3
		TIMES B , 0.90
	ELSEIF EXP:0 < EXPLV:4
		TIMES B , 0.80
	ELSEIF EXP:0 < EXPLV:5
		TIMES B , 0.70
	ELSE
		TIMES B , 0.60
	ENDIF
ELSE
	TIMES B , 1.75
	
	IF EXP:0 == 0
		IF ASSIPLAY && TALENT:PLAYER:122 == 0
			EXP:50 += 3
			PRINTFORML %EXPNAME:50%＋３
		ELSE
			EXP:50 += 2
			PRINTFORML %EXPNAME:50%＋２
		ENDIF
	ENDIF
ENDIF

SIF TALENT:PLAYER:121 || TALENT:PLAYER:122
	BASE:PLAYER:2 += B


;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 60
LOSEBASE:1 += 150

SOURCE:12 = 900

;上のほうで計算した汚れデータ
SOURCE:8 = Y*10 + 60

;ABL:奉仕精神をみる
IF ABL:6 == 0
	SOURCE:4 = 200
	SOURCE:5 = 50
	SOURCE:14 = 300
	TIMES SOURCE:8 , 4.00
ELSEIF ABL:6 == 1
	SOURCE:4 = 250
	SOURCE:5 = 200
	SOURCE:14 = 100
	TIMES SOURCE:8 , 2.50
ELSEIF ABL:6 == 2
	SOURCE:4 = 350
	SOURCE:5 = 550
	SOURCE:14 = 30
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:6 == 3
	SOURCE:4 = 450
	SOURCE:5 = 900
	SOURCE:14 = 0
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:6 == 4
	SOURCE:4 = 600
	SOURCE:5 = 1500
	SOURCE:14 = 0
	TIMES SOURCE:8 , 0.50
ELSE
	SOURCE:4 = 750
	SOURCE:5 = 2200
	SOURCE:14 = 0
	TIMES SOURCE:8 , 0.10
ENDIF

;ABL:Ｖ感覚をみる
IF ABL:4 == 0
	SOURCE:1 = 20
	SOURCE:3 = 25
	TIMES SOURCE:4 , 0.50
ELSEIF ABL:4 == 1
	SOURCE:1 = 75
	SOURCE:3 = 75
	TIMES SOURCE:4 , 0.80
ELSEIF ABL:4 == 2
	SOURCE:1 = 200
	SOURCE:3 = 125
	TIMES SOURCE:4 , 1.00
ELSEIF ABL:4 == 3
	SOURCE:1 = 500
	SOURCE:3 = 175
	TIMES SOURCE:4 , 1.20
ELSEIF ABL:4 == 4
	SOURCE:1 = 850
	SOURCE:3 = 300
	TIMES SOURCE:4 , 1.50
ELSE
	SOURCE:1 = 1100
	SOURCE:3 = 425
	TIMES SOURCE:4 , 2.00
ENDIF

;EXP:Ｖ経験をみる
IF EXP:0 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 = 5000
	SOURCE:12 = 12000
	SOURCE:14 = 120000
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 = 220
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:6 = 30
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:6 = 5
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:1 , 1.40
ELSE
	TIMES SOURCE:1 , 1.50
ENDIF


;PALAM:潤滑
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 += 900
	TIMES SOURCE:6 , 3.00
	SOURCE:12 += 2000
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 += 250
	TIMES SOURCE:6 , 1.00
	SOURCE:12 += 400
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:6 , 0.30
ELSEIF PALAM:3 < PALAMLV:5
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:6 , 0.20
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:6 , 0.10
ENDIF

;小柄体形
SIF TALENT:100
	TIMES SOURCE:6 , 2.00

;貞操観念
IF TALENT:30
	IF EXP:0 == 0
		TIMES SOURCE:3 , 0.60
		TIMES SOURCE:14, 15.00
		SOURCE:15 = 10000
	ELSE
		TIMES SOURCE:3 , 0.60
		TIMES SOURCE:14, 15.00
		SOURCE:15 = 1000
	ENDIF
;貞操無頓着
ELSEIF TALENT:31
	IF EXP:0 == 0
		TIMES SOURCE:14, 0.50
		SOURCE:15 = 300
	ELSE
		TIMES SOURCE:14, 0.50
	ENDIF
ELSE
	IF EXP:0 == 0
		SOURCE:15 = 3000
	ENDIF
ENDIF

;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:3 , 0.30
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.60
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:3 , 1.50
ELSEIF PALAM:5 < PALAMLV:5
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:3 , 1.80
ELSE
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:3 , 2.20
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:3 , 0.60
	TIMES SOURCE:15 , 2.00
	SOURCE:13 += 700
ELSEIF ABL:0 == 1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.80
	TIMES SOURCE:15 , 1.50
	SOURCE:13 += 500
ELSEIF ABL:0 == 2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:15 , 1.00
	SOURCE:13 += 300
ELSEIF ABL:0 == 3
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:15 , 0.80
	SOURCE:13 += 100
ELSEIF ABL:0 == 4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:3 , 1.40
	TIMES SOURCE:15 , 0.60
	SOURCE:13 += 30
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:3 , 1.60
	TIMES SOURCE:15 , 0.30
	SOURCE:13 += 0
ENDIF

;ABL:技巧をみる
IF ABL:2 == 0
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:3 , 0.60
ELSEIF ABL:2 == 1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.80
ELSEIF ABL:2 == 2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF ABL:2 == 3
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:3 , 1.20
ELSEIF ABL:2 == 4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:3 , 1.40
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:3 , 1.60
ENDIF


EXP:0 += 1
PRINTL Ｖ経験＋１

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
S = BASE:PLAYER:2
EJAC = MAXBASE:PLAYER:2

;天然オナホ
IF ((S > EJAC) && TALENT:107)
	BASE:PLAYER:2 = BASE:PLAYER:2 * 2
	S = BASE:PLAYER:2
ENDIF

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

IF E
;射精している
	TIMES SOURCE:4 , 3.00
	TIMES SOURCE:5 , 5.00
	TIMES SOURCE:13 , 1.50

	;ABL:精液中毒をみる
	IF ABL:11 == 0
		SOURCE:7 = 0
	ELSEIF ABL:11 == 1
		SOURCE:7 = 300
	ELSEIF ABL:11 == 2
		SOURCE:7 = 1000
	ELSEIF ABL:11 == 3
		SOURCE:7 = 1500
	ELSEIF ABL:11 == 4
		SOURCE:7 = 2200
	ELSE
		SOURCE:7 = 3000
	ENDIF
ENDIF

;大量射精
IF E == 2
	TIMES SOURCE:7 , 1.50
	TIMES SOURCE:5 , 1.20

	EXP:PLAYER:3 += 2
	EXP:20 += 2
	PRINTL 大量射精
	PRINTL 精液経験＋２
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC*2
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1
	;セックスで射精フラグ
	IF ASSIPLAY == 0
		CFLAG:500 = CFLAG:500 + 2
		CFLAG:MASTER:508 = CFLAG:MASTER:508 + 2
	ENDIF
	TFLAG:2 += 2
	
	CALL EJAC_IN_TARGET_VAGINA
;通常の射精
ELSEIF E == 1
	EXP:PLAYER:3 += 1
	EXP:20 += 1
	PRINTL 射精
	PRINTL 精液経験＋１
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1
	;セックスで射精フラグ
	IF ASSIPLAY == 0
		CFLAG:500 = CFLAG:500 + 1
		CFLAG:MASTER:508 = CFLAG:MASTER:508 + 1
	ENDIF
	TFLAG:2 += 1
	
	CALL EJAC_IN_TARGET_VAGINA
ENDIF

STAIN:3 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:3

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +7
	EXP:40 += 7
	SIF PLAYER != MASTER
		EXP:PLAYER:40 += 1
ENDIF

TFLAG:100 = 1

IF EXP:0 <= 1
	;屈服刻印３に相当
	TFLAG:200 = 3
ELSE
	;屈服刻印２に相当
	TFLAG:200 = 2
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:121
	SOURCE:13 /= 2
ENDIF

RETURN 1


