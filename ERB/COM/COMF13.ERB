@COM13
TSTR:0 = アナルバイブ
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM
;アナルバイブ
LOSEBASE:0 += 60
LOSEBASE:1 += 150

;ABL:Ａ感覚をみる
IF ABL:5 == 0
	SOURCE:2 = 80
	SOURCE:13 = 300
ELSEIF ABL:5 == 1
	SOURCE:2 = 250
	SOURCE:13 = 800
ELSEIF ABL:5 == 2
	SOURCE:2 = 600
	SOURCE:13 = 1400
ELSEIF ABL:5 == 3
	SOURCE:2 = 1000
	SOURCE:13 = 1800
ELSEIF ABL:5 == 4
	SOURCE:2 = 1300
	SOURCE:13 = 2100
ELSE
	SOURCE:2 = 1700
	SOURCE:13 = 2400
ENDIF

;EXP:Ａ経験をみる
IF EXP:1 < EXPLV:1
	TIMES SOURCE:2 , 0.50
	SOURCE:6 = 2000
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:2 , 1.00
	SOURCE:6 = 300
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:2 , 1.10
	SOURCE:6 = 50
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:2 , 1.20
	SOURCE:6 = 10
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:2 , 1.40
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:2 , 1.60
	SOURCE:6 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:6 += 800
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:6 += 500
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:6 += 300
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	SOURCE:6 += 120
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES SOURCE:2 , 1.80
	SOURCE:6 += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:2 , 0.80
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:2 , 0.90
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:2 , 1.10
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:2 , 1.20
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES SOURCE:2 , 0.80
ELSEIF ABL:0 == 1
	TIMES SOURCE:2 , 0.90
ELSEIF ABL:0 == 2
	TIMES SOURCE:2 , 1.00
ELSEIF ABL:0 == 3
	TIMES SOURCE:2 , 1.10
ELSEIF ABL:0 == 4
	TIMES SOURCE:2 , 1.20
ELSE
	TIMES SOURCE:2 , 1.30
ENDIF

;小柄体形
SIF TALENT:100
	TIMES SOURCE:6 , 2.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:106
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:13 , 1.50
	TIMES SOURCE:14 , 1.50
ELSEIF TALENT:105
	TIMES SOURCE:6 , 0.60
	TIMES SOURCE:13 , 0.60
	TIMES SOURCE:14 , 0.60
ENDIF

IF EXP:0 == 0 && TALENT:30
	SOURCE:13 /= 3
ENDIF

EXP:1 += 2
PRINTL Ａ経験＋２

;アナルバイブの着脱
IF TEQUIP:3
	TEQUIP:3 = 0
ELSE
	TEQUIP:3 = 1
	TEQUIP:5 = 0
	TEQUIP:6 = 0
ENDIF
CALL DISPLAY_EQUIP_MESSAGE_COM_SLAVE, 3
CALL DISPLAY_EQUIP_MESSAGE_COM_ASSI, 3

RETURN 1



