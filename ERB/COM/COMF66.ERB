@COM66
;Ｗフェラ

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:1
	SIF S
		PRINT  + 
	A += ABL:1 * 1
	PRINTS ABLNAME:1
	PRINTV 'LV,ABL:1,'(,ABL:1 * 1,')
	S = 1
ENDIF
;ABL:奉仕精神
IF ABL:6
	SIF S
		PRINT  + 
	A += ABL:6 * 4
	PRINTS ABLNAME:6
	PRINTV 'LV,ABL:6,'(,ABL:6 * 4,')
	S = 1
ENDIF
;ABL:精液中毒
IF ABL:11
	SIF S
		PRINT  + 
	A += ABL:11 * 3
	PRINTS ABLNAME:11
	PRINTV 'LV,ABL:11,'(,ABL:11 * 3,')
	S = 1
ENDIF

IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 1
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 1,')
	S = 1
ENDIF


;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 1
	PRINTS PALAMNAME:5
	PRINTV 'LV,L,'(,L * 1,')
	S = 1
ENDIF

;恥じらい
IF TALENT:35
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:35
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭鈍感
IF TALENT:61
	SIF S
		PRINT  + 
	A += 1
	PRINTS TALENTNAME:61
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭敏感
IF TALENT:62
	PRINT  - 
	A -= 3
	PRINTS TALENTNAME:62
	PRINTV '(,3,')
	S = 1
ENDIF
;献身的
IF TALENT:63
	SIF S
		PRINT  + 
	A += 6
	PRINTS TALENTNAME:63
	PRINTV '(,6,')
	S = 1
ENDIF
;快感の否定
IF TALENT:71
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:71
	PRINTV '(,1,')
	S = 1
ENDIF
;男嫌い
IF TALENT:82 && TALENT:PLAYER:122
	PRINT  - 
	A -= 12
	PRINTS TALENTNAME:82
	PRINTV '(,12,')
	S = 1
ENDIF
;恋慕
IF TALENT:85 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:85
	PRINTV '(,5,')
	S = 1
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:121
	SIF S
		PRINT  + 
	A += 8
	PRINTS TALENTNAME:121
	PRINTV '(,8,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
;血液汚れ
SIF STAIN:PLAYER:2 & 16
	Y += 5

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2



;汚れあり
IF Y
	PRINT  - 
	A -= Y
	;汚臭鈍感
	IF TALENT:61
		PRINT 汚れあり、
		PRINTS TALENTNAME:61
	ELSEIF TALENT:62
		PRINT 汚れあり、
		PRINTS TALENTNAME:62
	ELSE
		PRINT 汚れあり
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(24以上で実行)
PRINT  = 
PRINTV A

V = 24
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT

;実行できない
IF A < V
	CALL DISPLAY_KOJO_DENIAL_MESSAGE_COM, 31
	RETURN 0
ENDIF

;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = Ｗフェラ
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
B = 0

;AABL:技巧をみる
SELECTCASE ABL:2
	CASE 0
		B += 900
	CASE 1
		B += 1275
	CASE 2
		B += 1725
	CASE 3
		B += 2250
	CASE 4
		B += 2700
	CASE 5
		B += 3150
ENDSELECT

;ASSIのABL:技巧をみる
SELECTCASE ABL:ASSI:2
	CASE 0
		B += 900
	CASE 1
		B += 1275
	CASE 2
		B += 1725
	CASE 3
		B += 2250
	CASE 4
		B += 2700
	CASE 5
		B += 3150
ENDSELECT

;ABL:従順をみる
SELECTCASE ABL:0
	CASE 0
		TIMES B , 0.80
	CASE 1
		TIMES B , 0.90
	CASE 2
		TIMES B , 0.95
	CASE 3
		TIMES B , 1.00
	CASE 4
		TIMES B , 1.10
	CASE 5
		TIMES B , 1.20
ENDSELECT

;ASSIのABL:従順をみる
SELECTCASE ABL:ASSI:0
	CASE 0
		TIMES B , 0.80
	CASE 1
		TIMES B , 0.90
	CASE 2
		TIMES B , 0.95
	CASE 3
		TIMES B , 1.00
	CASE 4
		TIMES B , 1.10
	CASE 5
		TIMES B , 1.25
ENDSELECT

;ABL:奉仕精神をみる
SELECTCASE ABL:6
	CASE 0
		TIMES B , 0.75
	CASE 1
		TIMES B , 0.90
	CASE 2
		TIMES B , 1.10
	CASE 3
		TIMES B , 1.25
	CASE 4
		TIMES B , 1.50
	CASE 5
		TIMES B , 1.75
ENDSELECT

;ASSIのABL:奉仕精神をみる
SELECTCASE ABL:ASSI:6
	CASE 0
		TIMES B , 0.80
	CASE 1
		TIMES B , 0.95
	CASE 2
		TIMES B , 1.10
	CASE 3
		TIMES B , 1.25
	CASE 4
		TIMES B , 1.50
	CASE 5
		TIMES B , 1.75
ENDSELECT

;精液中毒をみる
SELECTCASE ABL:11
	CASE 0
		TIMES B , 0.90
	CASE 1
		TIMES B , 1.10
	CASE 2
		TIMES B , 1.25
	CASE 3
		TIMES B , 1.40
	CASE 4
		TIMES B , 1.60
	CASE 5
		TIMES B , 1.80
ENDSELECT

;精液中毒をみる
SELECTCASE ABL:ASSI:11
	CASE 0
		TIMES B , 0.90
	CASE 1
		TIMES B , 1.10
	CASE 2
		TIMES B , 1.25
	CASE 3
		TIMES B , 1.40
	CASE 4
		TIMES B , 1.60
	CASE 5
		TIMES B , 1.80
ENDSELECT

;TALENT:舌使いをみる
SIF TALENT:52
	TIMES B , 2.00
	
;ASSIのTALENT:舌使いをみる
SIF TALENT:ASSI:52
	TIMES B , 2.00

;プレイヤーのABL:Ｃ感覚をみる
SELECTCASE ABL:MASTER:3
	CASE 0
		TIMES B, 1.00
	CASE 1
		TIMES B, 1.25
	CASE 2
		TIMES B, 1.75
	CASE 3
		TIMES B, 2.50
	CASE 4
		TIMES B, 3.50
	CASE 5
		TIMES B, 4.75
ENDSELECT

SIF TALENT:PLAYER:121 || TALENT:PLAYER:122
	BASE:PLAYER:2 += B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 10
LOSEBASE:1 += 150

SOURCE:13 = 1500
SOURCE:14 = 500

;上のほうで計算した汚れデータ
SOURCE:8 = Y * 40 + 100

;ABL:奉仕精神をみる
SELECTCASE ABL:6
	CASE 0
		SOURCE:4 = 420
		SOURCE:5 = 150
		TIMES SOURCE:8 , 2.00
	CASE 1
		SOURCE:4 = 500
		SOURCE:5 = 300
		TIMES SOURCE:8 , 1.50
	CASE 2
		SOURCE:4 = 580
		SOURCE:5 = 600
		TIMES SOURCE:8 , 1.15
	CASE 3
		SOURCE:4 = 660
		SOURCE:5 = 900
		TIMES SOURCE:8 , 1.00
	CASE 4
		SOURCE:4 = 740
		SOURCE:5 = 1500
		TIMES SOURCE:8 , 0.50
	CASE 5
		SOURCE:4 = 820
		SOURCE:5 = 2200
		TIMES SOURCE:8 , 0.10
ENDSELECT

;ABL:技巧をみる
IF ABL:2 == 0
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:2 == 1
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:2 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:2 == 3
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:2 == 4
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSE
	TIMES SOURCE:4 , 2.00
	TIMES SOURCE:5 , 2.00
ENDIF

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
S = BASE:PLAYER:2
EJAC = MAXBASE:PLAYER:2

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

IF E
;射精している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:11 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 4.00
	ELSEIF ABL:11 == 1
		SOURCE:7 = 500
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 3.00
	ELSEIF ABL:11 == 2
		SOURCE:7 = 1200
		TIMES SOURCE:5 , 4.00
		TIMES SOURCE:13, 2.50
	ELSEIF ABL:11 == 3
		SOURCE:7 = 3000
		TIMES SOURCE:5 , 6.00
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:11 == 4
		SOURCE:7 = 6000
		TIMES SOURCE:5 , 9.00
		TIMES SOURCE:13, 1.50
	ELSE
		SOURCE:7 = 12000
		TIMES SOURCE:5 , 15.00
		TIMES SOURCE:13, 1.00
	ENDIF
ENDIF

;大量射精
IF E == 2
	TIMES SOURCE:7 , 2.00
	TIMES SOURCE:5 , 1.50

	EXP:PLAYER:3 += 2
	EXP:20 += 9
	PRINTL 大量射精
	PRINTL 精液経験＋９
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC*2
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;口で射精させたフラグ
	TFLAG:0 = 2
	
	;射精回数
	TFLAG:2 += 2
;通常の射精
ELSEIF E == 1
	EXP:PLAYER:3 += 1
	EXP:20 += 3
	PRINTL 射精
	PRINTL 精液経験＋３
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;口で射精させたフラグ
	TFLAG:0 = 1
	
	;射精回数
	TFLAG:2 += 1
ENDIF

;経験
PRINTFORM %EXPNAME:22%＋１
EXP:22 += 1
EXP:ASSI:22 += 1

;奴隷の口⇔調教者のＰの汚れが移動
STAIN:0 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:0

;舐め取る
IF ABL:2 >= 2 && ABL:6 >= 2
	STAIN:PLAYER:2 = 2
ENDIF

;奉仕精神ＬＶ２以上、技巧ＬＶ２以上なら汚れをなめ取る
IF (ABL:6 >= 2 && ABL:2 >= 2) || (ABL:ASSI:6 >=2 && ABL:ASSI:2 >= 2)
	STAIN:PLAYER:2 = 2
ENDIF

IF TALENT:122 && TALENT:MASTER:122
	EXP:41 += 5
	EXP:MASTER:41 += 5
	PRINTFORML %EXPNAME:41%+5
ENDIF
IF TALENT:ASSI:122 && TALENT:MASTER:122
	EXP:ASSI:41 += 5
	EXP:MASTER:41 += 5
	PRINTFORML 助手の%EXPNAME:41%+5
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:121
	SOURCE:13 /= 2
ENDIF

TFLAG:100 = 1

;屈服刻印Lv1に相当
SIF TFLAG:200 < 1
	TFLAG:200 = 1

RETURN 1


